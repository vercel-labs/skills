{
  "name": "add-skill",
  "version": "1.0.23",
  "description": "Install agent skills onto coding agents (OpenCode, Claude Code, Codex, Cursor)",
  "type": "module",
  "bin": {
    "add-skill": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsx src/index.ts",
    "test": "tsx scripts/execute-tests.ts",
    "prepublishOnly": "npm run build",
    "format": "prettier --write 'src/**/*.ts' 'scripts/**/*.ts' 'tests/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'scripts/**/*.ts' 'tests/**/*.ts'",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.ts": "prettier --write",
    "scripts/**/*.ts": "prettier --write",
    "tests/**/*.ts": "prettier --write"
  },
  "keywords": [
    "cli",
    "agent-skills",
    "skills",
    "ai-agents",
    "amp",
    "antigravity",
    "claude-code",
    "clawdbot",
    "codex",
    "cursor",
    "droid",
    "gemini-cli",
    "github-copilot",
    "goose",
    "kilo",
    "kiro-cli",
    "opencode",
    "roo",
    "trae",
    "windsurf",
    "neovate"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vercel-labs/add-skill.git"
  },
  "homepage": "https://github.com/vercel-labs/add-skill#readme",
  "bugs": {
    "url": "https://github.com/vercel-labs/add-skill/issues"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^0.9.1",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "gray-matter": "^4.0.3",
    "simple-git": "^3.27.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.17.1"
}
